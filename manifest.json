{
  "manifest_version": 3,
  "name": "Stylesheet Links Extractor",
  "version": "1.0",
  "description": "Extracts all stylesheet links from a webpage and displays them in a textarea.",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "assets/images/icon16.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/content.js"
      ]
    }
  ]
}